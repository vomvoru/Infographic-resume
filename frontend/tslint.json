{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [true, "never"],
    "ordered-imports": false,
    "trailing-comma": [true, {
      "multiline": "always",
      "singleline": "never"
    }]
  },
  "rulesDirectory": []
}
